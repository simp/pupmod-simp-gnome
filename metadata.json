{
  "name": "simp-gnome",

  "version": "9.0.0",
  "author": "SIMP Team",
  "summary": "Provides useful settings to set up the GNOME desktop environment",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-gnome",
  "project_page": "https://github.com/simp/pupmod-simp-gnome",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "gnome",
    "desktop"
  ],
  "dependencies": [
    {
      "name": "simp/dconf",
      "version_requirement": ">= 0.0.1 < 2.0.0"
    },
    {
      "name": "simp/polkit",
      "version_requirement": ">= 6.1.0 < 7.0.0"
    },
    {
      "name": "simp/simplib",
      "version_requirement": ">= 3.10.0 < 5.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.9.0 < 7.0.0"
    }
  ],
  "operatingsystem_support": [
    {
