{
  "name": "simp-gnome",
  "version": "6.0.3",
  "author": "SIMP Team",
  "summary": "provides useful settings to set up GNOME.",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-gnome",
  "project_page": "https://github.com/simp/pupmod-simp-gnome",
  "issues_url": "https://github.com/simp/pupmod-simp-gnome/issues",
  "tags": [
    "simp",
    "gnome"
  ],
  "dependencies": [
    {
      "name": "simp/mozilla",
      "version_requirement": ">= 5.0.0 < 6.0.0"
    },
    {
      "name": "simp/polkit",
      "version_requirement": ">= 6.1.0 < 7.0.0"
    },
    {
      "name": "simp/simplib",
      "version_requirement": ">= 3.1.0 < 4.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.9.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.6.0 < 5.0.0"
    }
  ]
}
